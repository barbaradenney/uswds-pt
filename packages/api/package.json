    "@fastify/jwt": "^8.0.0",
    "@fastify/rate-limit": "9",
    "@types/bcrypt": "^5.0.0",
    "@uswds-pt/shared": "workspace:*",
    "bcrypt": "^5.1.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.33.0",
    "fastify": "^4.29.1",
    "fastify-plugin": "^4.5.1",
    "nanoid": "^5.0.0",
    "openai": "^5.12.0",
    "postgres": "^3.4.0"
